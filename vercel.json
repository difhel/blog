{
    "rewrites": [
      {
        "source": "/((?!ru|en).*)",
        "has": [
          {
            "type": "header",
            "key": "accept-language",
            "value": "(^ru|ru-RU)"
          }
        ],
        "destination": "/ru/:path*"
      },
      {
        "source": "/((?!ru|en).*)",
        "destination": "/en/:path*"
      }
    ]
  }
  