{
  "rewrites": [
    {
      "source": "/((?!ru|en).*)",
      "has": [
        {
          "type": "header",
          "key": "accept-language",
          "value": "^(ru|ru-RU|ru-.*)"
        }
      ],
      "destination": "/ru/:0"
    },
    {
      "source": "/((?!ru|en).*)",
      "destination": "/en/:0"
    }
  ]
}
